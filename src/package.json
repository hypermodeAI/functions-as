{
  "name": "@hypermode/functions-as",
  "version": "0.10.5",
  "description": "Hypermode library for AssemblyScript functions",
  "author": "Hypermode, Inc.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "ast run",
    "pretest": "ast build && tsc -p ./tests",
    "build:transform": "tsc -p ./transform",
    "prepare": "npm run build:transform",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "pretty:check": "prettier --check ."
  },
  "dependencies": {
    "@assemblyscript/wasi-shim": "^0.1.0",
    "@hypermode/models-as": "^0.2.2",
    "json-as": "^0.9.21",
    "xid-ts": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.14",
    "as-test": "0.3.3",
    "assemblyscript": "^0.27.29",
    "assemblyscript-prettier": "^3.0.1",
    "eslint": "^9.8.0",
    "prettier": "^3.3.3",
    "semver": "^7.6.3",
    "sync-fetch": "^0.5.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "visitor-as": "^0.11.4"
  },
  "overrides": {
    "assemblyscript": "$assemblyscript"
  },
  "exports": {
    "./transform": "./transform/lib/index.js",
    "./tools/assemblyscript-eslint": "./tools/assemblyscript-eslint.js"
  },
  "files": [
    "index.ts",
    "assembly/**/*.ts",
    "!assembly/**/__tests__",
    "bin/build-plugin.js",
    "plugin.asconfig.json",
    "transform/lib/*.js",
    "tools/**"
  ]
}
